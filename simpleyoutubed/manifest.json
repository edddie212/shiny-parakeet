{
  "name": "Simple Youtube Video Downloader",
  "description": "Download Youtube videos easy!",
  "version": "0.1",
  "author": "Chill Chill",
  "manifest_version": 2,
  "web_accessible_resources": ["src/youtubeel.js"],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch*"],
    "js": ["./src/loader.js"],
    "css": ["./src/youtubeel.css"]
    }
  ],
  "permissions": [
    "downloads"
  ]
}